  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Main via PNPM",
      "request": "launch",
      "runtimeArgs": [
        "start:dev"
      ],
      "runtimeExecutable": "pnpm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node",
      "preLaunchTask": "start Contexts dev",
      "postDebugTask": "stop Contexts dev"
    },
    {
      "name": "Launch Micro1 via PNPM",
      "request": "launch",
      "runtimeArgs": [
        "start-micro1:dev"
      ],
      "runtimeExecutable": "pnpm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node",
      "preLaunchTask": "start Micro1 dev",
      "postDebugTask": "stop Contexts dev"
    },
    {
      "name": "Launch Micro2 via PNPM",
      "request": "launch",
      "runtimeArgs": [
        "start-micro2:dev"
      ],
      "runtimeExecutable": "pnpm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node",
      "preLaunchTask": "start Micro2 dev",
      "postDebugTask": "stop Contexts dev"
    }
  ]
}